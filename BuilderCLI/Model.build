# --------------------------------------------------------------------------------------------------
# Model: IR Rangefinder
# --------------------------------------------------------------------------------------------------

port port-1
set mode 'power'
set direction 'input'
set voltage 'ttl'

port port-2
set mode 'power'
set direction 'output'
set voltage 'common'

port port-3
set mode 'analog'
set direction 'output'
set voltage 'ttl'

device ir-rangefinder-ttl
add port port-1
add port port-2
add port port-3

